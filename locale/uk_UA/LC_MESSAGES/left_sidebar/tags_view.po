#: ../../left_sidebar/tags_view.rst:64 ../../left_sidebar/tags_view.rst:71
msgid "Bird"
msgstr "Пташка"

#: ../../left_sidebar/tags_view.rst:66 ../../left_sidebar/tags_view.rst:73
msgid "Cheetah"
msgstr "Гепард"

#: ../../left_sidebar/tags_view.rst:67
